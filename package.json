{
  "name": "games",
  "version": "1.0.0",
  "description": "Video games I own, have beaten, or are in my backlog.",
  "source": "src/index.html",
  "private": true,
  "scripts": {
    "clean": "del dist/*",
    "check": "tsc --project tsconfig.json",
    "lint": "prettier --check . && npx eslint && npm run check",
    "test": "npm run lint && jest",
    "format": "prettier --write .",
    "prestart": "npm run clean && npm run lint",
    "start": "parcel",
    "prebuild": "npm run clean && npm run lint",
    "build": "parcel build --no-cache"
  },
  "author": "Alex Gibson",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "del-cli": "^6.0.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "jest": "^29.7.0",
    "parcel": "^2.14.1",
    "parcel-reporter-static-files-copy": "^1.5.3",
    "prettier": "^3.5.3",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
